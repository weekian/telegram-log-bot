{
    "name": "telegram-log-bot",
    "version": "0.1.0",
    "description": "Telegram bot for logging daily personal work done",
    "engines": {
        "node": "10.13.0",
        "npm": "6.4.1"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm test && npm run build",
            "pre-commit": "npm run lint"
        }
    },
    "main": "dist/index.js",
    "scripts": {
        "build": "babel src -d dist",
        "flow": "flow",
        "test": "mocha --require @babel/register --require @babel/polyfill tests/**/*.spec.js",
        "lint": "eslint src/**",
        "prettier": "prettier --check \"src/**/*.js\"",
        "dev": "npm run build && node -r dotenv/config dist/index.js",
        "start": "node dist/index.js",
        "heroku-postbuild": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/weekian/telegram-log-bot.git"
    },
    "keywords": [
        "telegram",
        "bot"
    ],
    "author": "Yeam Wee Kian",
    "license": "GPL-3.0-or-later",
    "bugs": {
        "url": "https://github.com/weekian/telegram-log-bot/issues"
    },
    "homepage": "https://github.com/weekian/telegram-log-bot#readme",
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/polyfill": "^7.2.5",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-flow": "^7.0.0",
        "@babel/register": "^7.0.0",
        "babel-eslint": "^10.0.1",
        "chai": "^4.2.0",
        "dotenv": "^6.2.0",
        "eslint": "^5.13.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-config-prettier": "^4.0.0",
        "eslint-plugin-flowtype": "^3.2.1",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-prettier": "^3.0.1",
        "flow-bin": "^0.92.1",
        "husky": "^1.3.1",
        "mocha": "^5.2.0",
        "prettier": "^1.16.4"
    },
    "dependencies": {
        "express": "^4.16.4",
        "morgan": "^1.9.1",
        "telegraf": "^3.27.0",
        "telegraf-session-redis": "^5.1.0"
    }
}
